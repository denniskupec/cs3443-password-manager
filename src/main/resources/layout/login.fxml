<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="passmanager.controller.AppController">
   <children>
   	<VBox alignment="center" layoutX="242.0" layoutY="230.0" spacing="10.0">
         <children>
            <PasswordField alignment="CENTER" prefHeight="25.0" prefWidth="304.0" promptText="Enter Password to Unlock">
               <font>
                  <Font size="14.0" />
               </font>
            </PasswordField>
            <Button mnemonicParsing="false" onAction="#processLogin" text="Unlock">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="14.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnNewUser" mnemonicParsing="false" onAction="#processLogin" prefHeight="30.0" prefWidth="95.0" text="New User" />
            <Button fx:id="showAccount" mnemonicParsing="false" onAction="#showAccounts" text="Accounts" />
         </children>
      </VBox>
   </children>
</AnchorPane>
